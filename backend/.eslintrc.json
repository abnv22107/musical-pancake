{
    "env": {
      "node": true,
      "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "rules": {
      "semi": ["error", "always"],
      "quotes": ["error", "double"],
      "linebreak-style": "off", // Disable linebreak-style rule
      "no-console": "off", // Disable no-console rule
      "eol-last": "off" // Disable eol-last rule
    }
  }